{
    "name": "elixir/elixir-dispatcher",
    "description": "Component event manager",
    "minimum-stability": "dev",
    "license": "BSD-3-Clause",
    "keywords": [
        "elixir",
        "dispatcher"
    ],
    "homepage": "https://github.com/cedtanghe/elixir-dispatcher",
    
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "theseer/phpdox": "^0.9.0",
        "squizlabs/php_codesniffer": "^2.7",
        "phploc/phploc": "^3.0",
        "pdepend/pdepend": "^2.2",
        "sebastian/phpcpd": "2.0.4",
        "theseer/fdomdocument": "1.6.1"
    },
    "autoload": {
        "psr-4": {
            "Elixir\\Dispatcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elixir\\Test\\Dispatcher\\": "tests/"
        }
    }
}
